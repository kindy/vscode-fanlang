{
  "name": "fanlang",
  "displayName": "Fanlang",
  "description": "Fanlang support",
  "version": "1.3.1",
  "publisher": "kindy",
  "license": "MIT",
  "engines": {
    "vscode": "^1.55.0"
  },
  "activationEvents": [
    "onLanguage:fan"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fan",
        "aliases": [
          "Fanlang"
        ],
        "extensions": [
          ".fan"
        ],
        "configuration": "./fanlang.language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fan",
        "scopeName": "source.fan",
        "path": "./fanlang.grammar.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kindy/vscode-fanlang.git"
  },
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "dependencies": {
    "vscode-languageserver": "^6.1.1",
    "vscode-languageclient": "^6.1.3",
    "vscode-languageserver-protocol": "3.15.3",
    "vscode-languageserver-textdocument": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "@types/vscode": "^1.55.0",
    "peggy": "^1.1.0",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  }
}
